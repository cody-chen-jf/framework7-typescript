{
  "name": "framework7-template-vue-webpack",
  "version": "2.0.0",
  "description": "Framework7 Vue Webpack starter app template",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "dev-build": "NODE_ENV=development webpack --config ./build/webpack.dev.js",
    "build": "NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "copy-fonts": "cpy node_modules/framework7-icons/fonts/*.* src/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} src/fonts",
    "postinstall": "npm run copy-fonts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framework7io/framework7-template-vue-webpack.git"
  },
  "keywords": [
    "ios",
    "webpack",
    "framework7",
    "vue",
    "vuejs",
    "material",
    "mobile",
    "app",
    "f7"
  ],
  "author": "Vladimir Kharlampidi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framework7io/framework7-template-vue-webpack/issues"
  },
  "homepage": "http://framework7.io/vue/",
  "dependencies": {
    "axios": "^0.19.0",
    "framework7": "^4.5.1",
    "framework7-icons": "^2.3.1",
    "framework7-vue": "^4.5.1",
    "lodash": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.14.1",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@types/lodash": "^4.14.141",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.6.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cpy-cli": "^2.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass-loader": "^7.3.1",
    "sass-resources-loader": "^2.0.1",
    "style-loader": "^1.0.0",
